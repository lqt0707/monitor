{
  "name": "monitor-server",
  "version": "1.0.0",
  "description": "轻量级监控数据接收后端系统",
  "author": "Monitor Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:ollama": "node scripts/test-ollama.js",
    "migration:generate": "npx typeorm migration:generate -d dist/src/database/data-source.js",
    "migration:create": "npx typeorm migration:create",
    "migration:run": "npx typeorm migration:run -d dist/src/database/data-source.js",
    "migration:revert": "npx typeorm migration:revert -d dist/src/database/data-source.js",
    "migration:show": "npx typeorm migration:show -d dist/src/database/data-source.js"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.5",
    "@langchain/community": "^0.3.53",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/typeorm": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/decompress": "^4.2.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "adm-zip": "^0.5.16",
    "bcrypt": "^6.0.0",
    "bull": "^4.11.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compressing": "^2.0.0",
    "compression": "^1.7.4",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.13",
    "decompress": "^4.2.1",
    "dotenv": "^16.4.5",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "langchain": "^0.2.0",
    "mysql2": "^3.6.0",
    "nest-winston": "^1.10.2",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "source-map": "^0.7.6",
    "swagger-ui-express": "^5.0.0",
    "tar": "^7.4.3",
    "typeorm": "^0.3.17",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.18",
    "@types/source-map": "^0.5.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
