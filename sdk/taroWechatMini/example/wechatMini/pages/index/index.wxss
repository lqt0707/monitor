.container {
  padding: 20rpx;
  background-color: #f5f5f5;
  min-height: 100vh;
}

.header {
  text-align: center;
  margin-bottom: 40rpx;
  padding: 30rpx 20rpx;
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  border-radius: 20rpx;
  color: white;
}

.title {
  display: block;
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}

.subtitle {
  display: block;
  font-size: 28rpx;
  opacity: 0.9;
}

.test-section {
  margin-bottom: 40rpx;
}

.m-unit {
  margin-bottom: 30rpx;
  padding: 30rpx;
  background-color: white;
  border-radius: 15rpx;
  box-shadow: 0 4rpx 12rpx rgba(0, 0, 0, 0.1);
}

.test-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 10rpx;
}

.test-desc {
  font-size: 26rpx;
  color: #666;
  margin-bottom: 20rpx;
}

.results-section {
  background-color: white;
  border-radius: 15rpx;
  padding: 30rpx;
  box-shadow: 0 4rpx 12rpx rgba(0, 0, 0, 0.1);
}

.results-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 2rpx solid #eee;
}

.results-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
}

.results-list {
  max-height: 600rpx;
  overflow-y: auto;
}

.result-item {
  padding: 20rpx;
  margin-bottom: 15rpx;
  background-color: #f8f9fa;
  border-radius: 10rpx;
  border-left: 6rpx solid #007aff;
}

.result-time {
  font-size: 22rpx;
  color: #999;
  margin-bottom: 8rpx;
}

.result-test {
  font-size: 28rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 8rpx;
}

.result-content {
  font-size: 26rpx;
  color: #666;
  word-break: break-all;
}

.no-results {
  text-align: center;
  padding: 60rpx 20rpx;
  color: #999;
  font-size: 28rpx;
}